{
	"name": "dnadesign/ratingfeedback",
	"type": "silverstripe-vendormodule",
	"description": "Display a feedback form on any page",
	"license": "BSD-3-Clause",
	"keywords": ["silverstripe", "cms", "feedback", "rating", "stars"],
	"authors": [
		{
			"name": "DNA Design",
			"email": "dev@dna.co.nz"
		}
	],
	"repositories": [
		{
            "type": "vcs",
            "url": "git@github.com:izzudin96/silverstripe-subsite-modeladmins.git"
        }
	],
	"require": {
		"silverstripe/framework": "^4 || ^5",
		"silverstripe/cms": "^4 || ^5",
		"silverstripe/admin": "^2.2",
		"silverstripe/siteconfig": "^5.2",
		"adrexia/subsite-modeladmins": "dev-master"
	},
	"extra": {
		"expose": [
			"js",
			"css"
		]
	},
	"config": {
		"allow-plugins": {
			"composer/installers": false,
			"silverstripe/vendor-plugin": false
		}
	}
}
